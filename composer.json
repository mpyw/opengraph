{
    "name": "mpyw/opengraph",
    "description": "PHP library for consuming and publishing Open Graph resources.",
    "keywords": ["opengraph", "ogp"],
    "type": "library",
    "homepage": "https://github.com/mpyw/opengraph",
    "license": "MIT",
    "authors": [
        {
            "name": "Fusonic",
            "homepage": "http://www.fusonic.net"
        },
        {
            "name": "mpyw",
            "homepage": "https://github.com/mpyw"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mpyw\\OpenGraph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\OpenGraph\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "symfony/dom-crawler": "^4.1",
        "symfony/css-selector": "^4.1",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "friendsofphp/php-cs-fixer": "^2.15"
    }
}
