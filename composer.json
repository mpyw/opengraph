{
    "name": "mpyw/opengraph",
    "description": "PHP library for consuming and publishing Open Graph resources.",
    "keywords": ["opengraph", "ogp"],
    "type": "library",
    "homepage": "https://github.com/mpyw/opengraph",
    "license": "MIT",
    "authors": [
        {
            "name": "Fusonic",
            "homepage": "http://www.fusonic.net"
        },
        {
            "name": "mpyw",
            "homepage": "https://github.com/mpyw"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mpyw\\OpenGraph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\OpenGraph\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^5.4|^7.0",
        "symfony/dom-crawler": "^2.5",
        "symfony/css-selector": "^2.5",
        "guzzlehttp/guzzle": "^4.0"
    },
    "require-dev": {
        "fusonic/linq": "~1.0",
        "phpunit/phpunit": "^4.8|^5.7"
    }
}
